   "source": [
    "import calendar\n",
    "from datetime import date, timedelta\n",
    "\n",
    "dates = [(date.today() + timedelta(days=x)) for x in range((date(date.today().year, 12, 31) - date.today()).days + 1)]\n",
    "\n",
    "for d in dates:\n",
    "    print(f'{d.day}/{d.month}/{d.year} - {calendar.day_name[d.weekday()]}')"
   ]
  }
